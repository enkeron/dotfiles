! urxvt
!urxvt*background:                   #555244
!urxvt*foreground:                    white
!#include "/home/enkeron/.colors/erosion"
#include "/home/enkeron/.colors/jellybean"
!URxvt*termName: rxvt-unicode-256color
!urxvt*font:             xft:DejaVu Sans Mono:pixelsize=12
urxvt*font:             xft:Monaco:pixelsize=12
!urxvt*font:             xft:ProFont:pixelsize=15
!urxvt*font:             xft:GohuFont:pixelsize=15
!urxvt*font: xft:Terminus
!urxvt*boldFont: xft:Terminus
URxvt.perl-ext-common:default,matcher

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.rgba: rgb

URxvt*saveLines: 32767
URxvt.colorUL: #a9c9bc
!URxvt*cursorColor: white
!URxvt*cursorBlink: true
!URxvt*geometry: 90x26
!URxvt*title: rxvt-256color
URxvt*title:Terminal 

URxvt*jumpScroll: true
URxvt*secondaryScroll: true
! jwr dark (slightly modified)
URxvt.background: #080808
URxvt.foreground: #c7c7c7


URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
!URxvt.colorUL: #395573
!URxvt.perl-ext: default,url-select
!URxvt.keysym.M-u: perl:url-select:select_next
!URxvt.url-select.launcher: /usr/bin/xdg-open
!URxvt.url-select.underline: true

!*background: #002b36
!*foreground: #657b83

!!*fading: 40
*fadeColor: #002b36
*cursorColor: #93a1a1
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

URxvt*buffered: true
!URxvt*cursorBlink: false
!URxvt*cursorUnderlline: false
URxvt*geometry: 80x25
URxvt.iconFile:/usr/share/icons/Mint-X/apps/scalable/terminal.svg
URxvt*scrollBar: false
URxvt*scrollstyle: plain
URxvt*scrollWithBuffer: false
URxvt*scrollTtyOutput: true
URxvt*pointerBlank: true

